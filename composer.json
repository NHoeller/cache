{
    "name": "neos/cache",
    "type": "neos-framework",
    "description": "Flow Cache Framework",
    "homepage": "http://flow.neos.io",
    "license": ["MIT"],
    "require": {
        "php": ">=5.5.0",

        "ext-zlib": "*",
        "ext-SPL": "*",
        "ext-json": ">=1.2.0",
        "ext-reflection": "*",
        "typo3/flow": "*"
    },
    "autoload": {
        "psr-0": {
            "TYPO3\\Flow\\Cache": "Classes"
        },
        "psr-4": {
            "Neos\\Cache\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\Flow\\Cache\\Tests\\": "Tests"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Neos.Cache"
        }
    }
}
